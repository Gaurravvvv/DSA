class Solution {
public:
    void rotate(vector<int>& nums, int k) {

        int n = nums.size() ;
        k = k%n ;
        vector<int> result ;

        for(int i = n-k ; i<n ; i++){
            result.push_back(nums[i]) ; // Add the elements from last to new array
        }
        for(int i = 0 ; i<n-k ; i++){
            result.push_back(nums[i]) ; // Add the first elements after last elements

        }

        nums = result ; // assign back new vector values to orignal vector 

    }  
};
