class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {

        int n = nums.size() ;
        int cnt = 0 ;
        int cnt1 = 0 ;

        for(int i = 0 ; i<n ; i++){
            if(nums[i]==1){
                cnt++ ;
                cnt1 = max(cnt , cnt1) ;

            }
            else{
                cnt = 0 ;
            }
        }
        return cnt1 ;

        
    }
};
